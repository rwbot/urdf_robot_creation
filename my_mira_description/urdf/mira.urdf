<?xml version="1.0"?>
<robot name="mira">
<!--URDF uses SI Units: meters, radians and kilograms-->
<!-- Three basic geometry shapes:
<cylinder radius="meters" length="meters"/>
<box size="x_length y_length z_length"/>
<sphere radius="meters"/>
-->

<!--
rosservice call /gazebo/delete_model "model_name: 'mira'"
roslaunch my_mira_description spawn_mira.launch" 
-->
    

    <!--This activates the gazebo control plugin for the Mira namespace. This namespace will be set in the main spawn launch file.-->
    <gazebo>
        <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
            <robotNamespace>/mira</robotNamespace>
        </plugin>
    </gazebo>


